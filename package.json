    "scripts": {
        "build": "tsc -p tsconfig.build.json",
        "format": "prettier --write \"src/**/*.ts\"",
        "start:ts": "export TS_NODE=true && ts-node -r tsconfig-paths/register src/main.ts",
        "start:test": "node -r tsconfig-paths/register dist/main.js",
        "start:dev": "export TS_NODE=true && nodemon",
        "start:debug": "nodemon --config nodemon-debug.json",
        "start": "rimraf dist && npm run build && node -r ./tsconfig-paths-bootstrap.js dist/main.js",
        "documentation": "npx compodoc -r 8000 -p tsconfig.json -s",
        "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
        "typeorm:migrate": "npm run typeorm migration:generate -- -n",
        "typeorm:run": "npm run typeorm migration:run",
        "lint": "tslint -p tsconfig.json -c tslint.json",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "test:e2e": "jest --config ./test/jest-e2e.json"
    },
    "dependencies": {
        "@nest/microservices": "^4.5.4",
        "@nestjs/common": "^6.3.0",

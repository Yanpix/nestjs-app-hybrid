        "@nestjs/jwt": "^6.0.0",
        "@nestjs/microservices": "^6.5.1",
        "@nestjs/mongoose": "^6.1.2",
        "@nestjs/passport": "^6.0.0",
        "@nestjs/platform-express": "^6.0.0",
        "@nestjs/swagger": "^3.1.0",
        "@nestjs/typeorm": "^6.0.0",
        "@sendgrid/mail": "^6.4.0",
        "@types/multer": "^1.3.7",
        "@types/xml2js": "^0.4.5",
        "app-root-path": "^2.2.1",
        "aws-sdk": "^2.468.0",
        "axios": "^0.19.0",
        "class-transformer": "^0.3.1",
        "class-validator": "^0.9.1",
        "cookie-parser": "^1.4.4",
        "csurf": "^1.10.0",
        "currency-formatter": "^1.5.5",
        "ejs": "^2.6.1",
        "exceljs": "^1.13.0",
        "express-basic-auth": "^1.2.0",
        "express-rate-limit": "^3.4.1",
        "express-session": "^1.16.1",
        "helmet": "^3.21.1",
        "html-pdf": "^2.2.0",
        "moment": "^2.24.0",
        "moment-timezone": "^0.5.26",
        "mongoose": "^5.6.6",
        "multer": "^1.4.1",

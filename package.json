        "nestjs-config": "^1.3.21",
        "passport": "^0.4.0",
        "passport-jwt": "^4.0.0",
        "passport-local": "^1.0.0",
        "reflect-metadata": "^0.1.12",
        "rimraf": "^2.6.2",
        "rxjs": "^6.3.3",
        "swagger-ui-express": "^4.0.3",
        "translate-google": "^1.3.5",
        "typeorm": "^0.2.16",
        "uniqid": "^5.0.3",
        "xml2js": "^0.4.22"
    },
    "devDependencies": {
        "@compodoc/compodoc": "^1.1.10",
        "@nestjs/testing": "^6.5.3",
        "@types/express": "^4.16.0",
        "@types/jest": "^23.3.13",
        "@types/node": "^10.12.18",
        "@types/supertest": "^2.0.7",
        "chai": "^4.2.0",
        "jest": "^24.7.1",
        "nodemon": "^1.18.9",
        "prettier": "^1.15.3",
        "sinon": "^7.4.1",
        "supertest": "^3.4.1",
        "ts-jest": "^23.10.5",
        "ts-node": "^7.0.1",
        "tsconfig-paths": "^3.8.0",
        "tslint": "5.12.1",
        "typescript": "^3.2.4"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "rootDir": "src",
        "testRegex": ".spec.ts$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "coverageDirectory": "../coverage",
        "testEnvironment": "node"
    }
}
